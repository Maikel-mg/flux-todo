{
  "name": "es6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js"
  },
  "author": "Maikel Merillas",
  "browserify": {
    "transform": [
      "reactify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "envify"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "envify": "^3.4.0",
    "reactify": "^1.1.1",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "events": "^1.1.0",
    "flux": "^2.1.1",
    "react": "^0.14.5",
    "react-dom": "^0.14.5"
  }
}
